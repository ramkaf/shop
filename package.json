    "migrate:apply": "npx prisma migrate dev",
    "migrate:dev": "npm run migrate:create && npm run migrate:apply",
    "migrate:status": "npx prisma migrate status",
    "create:model": "node ./scripts/createModel.cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npx prettier --write --no-comments .",
    "remove-comments": "powershell -ExecutionPolicy Bypass -File ./scripts/remove-comments.ps1",
    "remove-empty-lines": "powershell -ExecutionPolicy Bypass -File ./scripts/remove-empty-lines.ps1",
    "clean-and-install": "npm run remove-comments && npm run remove-empty-lines && npm install --save-dev @types/node"
  },
